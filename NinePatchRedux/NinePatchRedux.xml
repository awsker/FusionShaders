<effect>
<name>NinePatch Redux</name>
<author>asker</author>
<description>Create custom-scaled graphical boxes with ease</description>
<parameter>
		<name>X Scale factor</name>
		<code>xScale</code>
		<description>Used to determine non-scaled size.</description>
		<type>float</type>
		<property>spin</property>
		<value>1.0</value>
</parameter>
<parameter>
		<name>Y Scale factor</name>
		<code>yScale</code>
		<description>Used to determine non-scaled size.</description>
		<type>float</type>
		<property>spin</property>
		<value>1.0</value>
</parameter>
<parameter>
		<name>Repeat pattern</name>
		<code>repeat</code>
		<description>Enable to have the pattern repeat instead of stretch.</description>
		<type>int</type>
		<property>checkbox</property>
		<value>1</value>
</parameter>
</effect>
